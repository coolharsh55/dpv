dpv-gdpr:A45-3 a rdfs:Class,
        skos:Concept,
        dpv:DataTransferLegalBasis ;
    dct:contributor "Georg P Krog"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:modified "2021-09-08"^^xsd:date ;
    dct:source "(GDPR Art.45-3,https://eur-lex.europa.eu/eli/reg/2016/679/art_45/par_3/oj)"@en ;
    sw:term_status "accepted"@en ;
    skos:definition "Personal data can flow freely from the EU to a third country with an Adequacy Decision without any further safeguard being necessary."@en ;
    skos:note "Transfer from EU to a third country. Third country has Adequacy Decision."@en ;
    skos:prefLabel "Art 45(3) adequacy decision"@en .

