    <rdf:type rdf:resource="https://w3id.org/dpv#DataSubjectRight"/>
    <skos:prefLabel xml:lang="en">A17 Right to Erasure</skos:prefLabel>
    <skos:definition xml:lang="en">Right to erasure ('Right to be forgotten')</skos:definition>
    <dct:source xml:lang="en">GDPR Art.17,https://eur-lex.europa.eu/eli/reg/2016/679/art_17/oj)</dct:source>
