  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr#RightsRecipientsNotice">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#RightFulfilmentNotice"/>
    <skos:prefLabel xml:lang="en">Rights Recipients Notice</skos:prefLabel>
    <skos:definition xml:lang="en">A Notice provided in fulfilment of GDPR's Art.19 regarding Recipients to whom a rights exercise has been communicated, such as regarding rectification (A.16) or erasure of personal data (A.17) or restriction of processing (A.18)</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-11-09</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor xml:lang="en">Beatriz Esteves, Georg Krog, Harshvardhan J. Pandit</dct:contributor>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr#A19">
