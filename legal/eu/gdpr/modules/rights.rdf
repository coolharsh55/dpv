    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#DataSubjectRight"/>
    <skos:prefLabel xml:lang="en">A19 Right to Rectification</skos:prefLabel>
    <skos:definition xml:lang="en">Right to be notified in case of rectification or erasure of personal data or restriction of processing</skos:definition>
    <dct:source xml:lang="en">(GDPR Art.19,https://eur-lex.europa.eu/eli/reg/2016/679/art_19/oj)</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor xml:lang="en">Beatriz Esteves, Georg Krog, Harshvardhan J. Pandit</dct:contributor>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr#SARNotice">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#RightFulfilmentNotice"/>
    <skos:prefLabel xml:lang="en">SAR Notice</skos:prefLabel>
    <skos:definition xml:lang="en">A Notice provided in fulfilment of GDPR's Art.15 regarding information to be provided for Right of Access or Subject Access Request (SAR)</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-11-09</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor xml:lang="en">Beatriz Esteves, Georg Krog, Harshvardhan J. Pandit</dct:contributor>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/gdpr#A18">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#DataSubjectRight"/>
    <skos:prefLabel xml:lang="en">A18 Right to Restrict Processing</skos:prefLabel>
    <skos:definition xml:lang="en">Right to restriction of processing</skos:definition>
    <dct:source xml:lang="en">GDPR Art.18,https://eur-lex.europa.eu/eli/reg/2016/679/art_18/oj)</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor xml:lang="en">Beatriz Esteves, Georg Krog, Harshvardhan J. Pandit</dct:contributor>
