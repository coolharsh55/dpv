    skos:inScheme eu-gdpr:legal-basis-classes ;
    skos:narrower eu-gdpr:A6-1-e-official-authority,
        eu-gdpr:A6-1-e-public-interest ;
    skos:prefLabel "Art 6(1-e) public interest or official authority"@en .

eu-gdpr:A6-1-e-official-authority a rdfs:Class,
        skos:Concept,
        dpv:LegalBasis ;
    dct:source "(GDPR Art.6-1e,https://eur-lex.europa.eu/eli/reg/2016/679/art_6/par_1/pnt_e/oj)"@en ;
    sw:term_status "accepted"@en ;
