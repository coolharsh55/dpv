  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/dga#RightToImpartialReview">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#Right"/>
    <skos:prefLabel xml:lang="en">Right to Impartial Review</skos:prefLabel>
    <skos:definition xml:lang="en">Right of data subjects and data holders to get an review by an impartial body with the appropriate expertise</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#Right"/>
    <dct:source xml:lang="en">DGA 28.3</dct:source>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/legal/eu/dga#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/legal/eu/dga#legal-rights-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/legal/eu/dga">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <dct:title xml:lang="en">EU Data Governance Act (DGA)</dct:title>
    <dct:description xml:lang="en">Extension to the Data Privacy Vocabulary (DPV) providing concepts for representing  information associated with EU DGA</dct:description>
    <dct:created xml:lang="en">2023-09-20</dct:created>
    <dct:modified xml:lang="en">2024-01-01</dct:modified>
    <dct:creator xml:lang="en">Beatriz Esteves</dct:creator>
