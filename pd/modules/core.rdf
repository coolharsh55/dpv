    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#PersonalData"/>
    <skos:prefLabel xml:lang="en">Tracking</skos:prefLabel>
    <skos:definition xml:lang="en">Information used to track an individual or group e.g. location or email</skos:definition>
    <dct:source xml:lang="en">(EnterPrivacy Categories of Personal Information, https://enterprivacy.com/wp-content/uploads/2018/09/Categories-of-Personal-Information.pdf)</dct:source>
