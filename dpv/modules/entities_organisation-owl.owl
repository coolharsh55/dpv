  <rdf:Description rdf:about="https://w3id.org/dpv#ForProfitOrganisation">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">For-Profit Organisation</skos:prefLabel>
