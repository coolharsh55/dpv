    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#GovernmentalOrganisation">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Governmental Organisation</skos:prefLabel>
    <skos:definition xml:lang="en">An organisation managed or part of government</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#Organisation"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-02-02</dct:created>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-05</dct:modified>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#InternationalOrganisation">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">International Organisation</skos:prefLabel>
    <skos:definition xml:lang="en">An organisation and its subordinate bodies governed by public international law, or any other body which is set up by, or on the basis of, an agreement between two or more countries</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#Organisation"/>
    <dct:source xml:lang="en">(GDPR Art.4-26,https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_26/oj)</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-03-23</dct:created>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-05</dct:modified>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Julian Flake, Georg P. Krog</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <dct:title xml:lang="en">Data Privacy Vocabulary (DPV)</dct:title>
    <dct:description xml:lang="en">The Data Privacy Vocabulary (DPV) provides terms (classes and properties) to represent information about processing of personal data, for example - purposes, processing operations, personal data, technical and organisational measures.</dct:description>
    <dct:created xml:lang="en">2022-08-18</dct:created>
    <dct:modified xml:lang="en">2024-01-01</dct:modified>
    <dct:creator xml:lang="en">Harshvardhan J. Pandit</dct:creator>
    <schema:version>2</schema:version>
    <dct:identifier>https://w3id.org/dpv</dct:identifier>
    <dct:conformsTo>http://www.w3.org/2000/01/rdf-schema</dct:conformsTo>
    <dct:conformsTo>http://www.w3.org/2004/02/skos/core</dct:conformsTo>
    <dct:conformsTo rdf:resource="http://www.w3.org/2002/07/owl"/>
    <dct:contributor>Julian Flake</dct:contributor>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <dct:contributor>Paul Ryan</dct:contributor>
    <dct:contributor>Georg P. Krog</dct:contributor>
    <dct:license rdf:resource="https://www.w3.org/copyright/document-license-2023/"/>
    <vann:preferredNamespacePrefix>dpv</vann:preferredNamespacePrefix>
    <vann:preferredNamespaceUri>https://w3id.org/dpv#</vann:preferredNamespaceUri>
    <dct:hasVersion rdf:resource="https://w3id.org/dpv"/>
  </rdf:Description>
