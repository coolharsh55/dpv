    <skos:prefLabel xml:lang="en">Cloud Location</skos:prefLabel>
    <skos:definition xml:lang="en">Location that is in the 'cloud' i.e. a logical location operated over the internet</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#RemoteLocation"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#jurisdiction-classes"/>
  </rdf:Description>
