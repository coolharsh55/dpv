>
  <rdf:Description rdf:about="https://w3id.org/dpv#WithinVirtualEnvironment">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#Location"/>
    <skos:prefLabel xml:lang="en">Within Virtual Environment</skos:prefLabel>
    <skos:definition xml:lang="en">Location is local and entirely within a virtual environment, such as a shared network directory</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#LocalLocation"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#jurisdiction-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#Region">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#Location"/>
    <skos:prefLabel xml:lang="en">Region</skos:prefLabel>
    <skos:definition xml:lang="en">A region is an area or site that is considered a location</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#Location"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#jurisdiction-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#City">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#Location"/>
    <skos:prefLabel xml:lang="en">City</skos:prefLabel>
    <skos:definition xml:lang="en">A region consisting of urban population and commerce</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#Location"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#jurisdiction-classes"/>
  </rdf:Description>
