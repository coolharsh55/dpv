    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#jurisdiction-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#FederatedLocations">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#LocationFixture"/>
