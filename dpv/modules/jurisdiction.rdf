    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:narrower rdf:resource="https://w3id.org/dpv#LocalLocation"/>
    <skos:narrower rdf:resource="https://w3id.org/dpv#RemoteLocation"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#jurisdiction-classes"/>
