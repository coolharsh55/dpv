    <rdf:type rdf:resource="https://w3id.org/dpv#Location"/>
    <skos:prefLabel xml:lang="en">Local Location</skos:prefLabel>
    <skos:definition xml:lang="en">Location is local</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#LocationLocality"/>
