    <skos:note xml:lang="en">Context is a catch-all concept for information of relevance not possible to represent through other core concepts. DPV offers specific contextual concepts such as Necessity, Frequency, and Duration. More can be created by extending Context within use-cases.</skos:note>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:superClassOf rdf:resource="https://w3id.org/dpv#Importance"/>
    <rdfs:superClassOf rdf:resource="https://w3id.org/dpv#Necessity"/>
    <rdfs:superClassOf rdf:resource="https://w3id.org/dpv#Scope"/>
    <rdfs:superClassOf rdf:resource="https://w3id.org/dpv#Justification"/>
    <rdfs:superClassOf rdf:resource="https://w3id.org/dpv#Frequency"/>
    <rdfs:superClassOf rdf:resource="https://w3id.org/dpv#Duration"/>
    <skos:narrower rdf:resource="https://w3id.org/dpv#Importance"/>
    <skos:narrower rdf:resource="https://w3id.org/dpv#Necessity"/>
    <skos:narrower rdf:resource="https://w3id.org/dpv#Scope"/>
    <skos:narrower rdf:resource="https://w3id.org/dpv#Justification"/>
    <skos:narrower rdf:resource="https://w3id.org/dpv#Frequency"/>
    <skos:narrower rdf:resource="https://w3id.org/dpv#Duration"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#context-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#NotRequired">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#Necessity"/>
    <skos:prefLabel xml:lang="en">Not Required</skos:prefLabel>
    <skos:definition xml:lang="en">Indication of neither being required nor optional i.e. not relevant or needed</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#Necessity"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#context-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#OftenFrequency">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
