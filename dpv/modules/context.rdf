    <skos:note xml:lang="en">The term 'technology' is inclusive of technologies, processes, and methods.</skos:note>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#context-properties"/>
  </rdf:Description>
