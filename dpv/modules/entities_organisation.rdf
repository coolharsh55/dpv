  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#NonProfitOrganisation">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#Organisation"/>
    <skos:prefLabel xml:lang="en">Non-Profit Organisation</skos:prefLabel>
    <skos:definition xml:lang="en">An organisation that does not aim to achieve profit as its primary goal</skos:definition>
    <dct:source xml:lang="en">(ADMS controlled vocabulary,http://purl.org/adms)</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-02-02</dct:created>
