    <skos:narrower rdf:resource="https://w3id.org/dpv#InternationalOrganisation"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#entities-organisation-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#ForProfitOrganisation">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">For-Profit Organisation</skos:prefLabel>
    <skos:definition xml:lang="en">An organisation that aims to achieve profit as its primary goal</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#Organisation"/>
    <skos:broader rdf:resource="https://w3id.org/dpv#Organisation"/>
