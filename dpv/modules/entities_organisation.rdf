  <rdf:Description rdf:about="https://w3id.org/dpv#OrganisationalUnit">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Organisational Unit</skos:prefLabel>
    <skos:definition xml:lang="en">Entity within an organisation that does not constitute as a separate legal entity</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#Entity"/>
    <skos:broader rdf:resource="https://w3id.org/dpv#Entity"/>
