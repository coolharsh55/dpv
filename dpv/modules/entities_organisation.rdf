  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#IndustryConsortium">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#Organisation"/>
    <skos:prefLabel xml:lang="en">Industry Consortium</skos:prefLabel>
    <skos:definition xml:lang="en">A consortium established and comprising on industry organisations</skos:definition>
    <dct:source xml:lang="en">(ADMS controlled vocabulary,http://purl.org/adms)</dct:source>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-02-02</dct:created>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-05</dct:modified>
