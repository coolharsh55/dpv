      }
    ],
    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "@language": "en",
        "@value": "An entity that 'imports' data where importing is considered a form of data transfer"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#inScheme": [
