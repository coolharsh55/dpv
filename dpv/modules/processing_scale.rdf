    <rdfs:superClassOf rdf:resource="https://w3id.org/dpv#DataSubjectScale"/>
    <rdfs:superClassOf rdf:resource="https://w3id.org/dpv#GeographicCoverage"/>
    <rdfs:superClassOf rdf:resource="https://w3id.org/dpv#ProcessingScale"/>
    <skos:narrower rdf:resource="https://w3id.org/dpv#DataVolume"/>
