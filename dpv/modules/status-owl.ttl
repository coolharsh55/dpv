    skos:definition "Status associated with Compliance with some norms, objectives, or requirements"@en ;
    skos:prefLabel "Compliance Status"@en .

dpv:ComplianceUnknown a rdfs:Class,
        owl:Class,
        dpv:ComplianceStatus ;
