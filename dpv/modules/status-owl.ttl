    dct:created "2022-05-18"^^xsd:date ;
    rdfs:isDefinedBy dpv: ;
    rdfs:subClassOf dpv:Status ;
    rdfs:superClassOf dpv:ComplianceIndeterminate,
        dpv:ComplianceUnknown,
