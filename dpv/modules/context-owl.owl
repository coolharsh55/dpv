    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">has frequency</skos:prefLabel>
    <skos:definition xml:lang="en">Indicates the frequency with which something takes place</skos:definition>
    <dcam:rangeIncludes rdf:resource="https://w3id.org/dpv#Frequency"/>
