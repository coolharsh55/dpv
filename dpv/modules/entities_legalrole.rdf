    <skos:narrower rdf:resource="https://w3id.org/dpv#hasDataExporter"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#LegalEntity">
    <rdfs:superClassOf rdf:resource="https://w3id.org/dpv#DataController"/>
    <rdfs:superClassOf rdf:resource="https://w3id.org/dpv#Recipient"/>
    <rdfs:superClassOf rdf:resource="https://w3id.org/dpv#DataExporter"/>
    <skos:narrower rdf:resource="https://w3id.org/dpv#DataController"/>
    <skos:narrower rdf:resource="https://w3id.org/dpv#Recipient"/>
    <skos:narrower rdf:resource="https://w3id.org/dpv#DataExporter"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#hasRepresentative">
    <rdfs:superPropertyOf rdf:resource="https://w3id.org/dpv#hasDataProtectionOfficer"/>
    <skos:narrower rdf:resource="https://w3id.org/dpv#hasDataProtectionOfficer"/>
  </rdf:Description>
