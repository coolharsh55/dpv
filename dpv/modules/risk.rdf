    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#Impact"/>
    <skos:prefLabel xml:lang="en">Benefit</skos:prefLabel>
    <skos:definition xml:lang="en">Impact(s) that acts as or causes benefits</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv#Impact"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#risk-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#hasConsequence">
