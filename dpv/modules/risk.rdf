    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Impact</skos:prefLabel>
    <skos:definition xml:lang="en">The impact(s) possible or arising as a consequence from specified context</skos:definition>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#Consequence"/>
    <skos:broader rdf:resource="https://w3id.org/dpv#Consequence"/>
    <skos:note xml:lang="en">Impact is a stronger notion of consequence in terms of influence, change, or effect on something e.g. for impact assessments</skos:note>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:narrower rdf:resource="https://w3id.org/dpv#Benefit"/>
    <skos:narrower rdf:resource="https://w3id.org/dpv#Detriment"/>
    <skos:narrower rdf:resource="https://w3id.org/dpv#Damage"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#risk-classes"/>
  </rdf:Description>
