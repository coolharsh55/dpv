    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:contributor>Harshvardhan J. Pandit</dct:contributor>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv#HumanInvolvement"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#ProcessingCondition">
    <rdfs:superClassOf rdf:resource="https://w3id.org/dpv#StorageCondition"/>
    <rdfs:superClassOf rdf:resource="https://w3id.org/dpv#ProcessingLocation"/>
