  <rdf:Description rdf:about="https://w3id.org/dpv#DataTransferLegalBasis">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Data Transfer Legal Basis</skos:prefLabel>
    <skos:definition xml:lang="en">Specific or special categories and instances of legal basis intended for justifying data transfers</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
