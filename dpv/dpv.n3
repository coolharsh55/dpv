dpv:AuditConditionallyApproved a rdfs:Class,
        skos:Concept,
        dpv:AuditStatus ;
    dct:contributor "Paul Ryan"@en ;
    dct:created "2022-06-29"^^xsd:date ;
    sw:term_status "accepted"@en ;
