    <skos:narrower rdf:resource="https://w3id.org/dpv#OfficialAuthorityOfController"/>
    <skos:narrower rdf:resource="https://w3id.org/dpv#PublicInterest"/>
    <skos:narrower rdf:resource="https://w3id.org/dpv#VitalInterest"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv#legal-basis-classes"/>
  </rdf:Description>
