    skos:prefLabel "Controller-Processor Agreement"@en .

dpv:Copy a rdfs:Class,
        skos:Concept,
        dpv:Processing ;
    dct:source "(SPECIAL Project,https://specialprivacy.ercim.eu/vocabs/processing)"@en ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:Processing ;
    skos:definition "to produce an exact reprodution of the data"@en ;
    skos:prefLabel "Copy"@en ;
    skos:related "svpr:Copy"@en .

dpv:CounterMoneyLaundering a rdfs:Class,
        skos:Concept,
        dpv:Purpose ;
    sw:term_status "accepted"@en ;
