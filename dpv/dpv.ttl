    rdfs:superClassOf dpv:ActivityStatus,
        dpv:AuditStatus,
        dpv:ComplianceStatus,
        dpv:ConformanceStatus,
        dpv:ConsentStatus,
        dpv:RequestStatus ;
    sw:term_status "accepted"@en ;
    skos:definition "The status or state of something"@en ;
    skos:prefLabel "Status"@en .

