    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/tech#"/>
    <skos:inScheme rdf:resource="https://w3id.org/dpv/tech#surveillance-classes"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/tech#CovertSurveillanceTechnology">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#Technology"/>
    <skos:prefLabel xml:lang="en">Covert SurveillanceTechnology</skos:prefLabel>
    <skos:definition xml:lang="en">Surveillance that is covert i.e. invisible or non-apparent or implicit</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv/tech#SurveillanceTechnology"/>
    <skos:scopeNote xml:lang="en">For example, a web resource that performs tracking in the background</skos:scopeNote>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-15</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
